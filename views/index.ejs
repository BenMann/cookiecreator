<% include header %>

  <div id="pagewrap">
    <div id="header">
    </div>

    <div id="innerwrap">
      <div id="content">
      <h2>Create a new Package:</h2>
      <p>Just enter its name and URL</p>
      <form action="/create/" method="post">
        <div class="row">
          <label for="name">Name</label>
          <input type="text" autofocus="autofocus" name="name" id="name">
        </div>
        <div class="row">
          <label for="url">URL</label>
          <input type="text" name="url" id="url">
        </div>
        <input type="submit" value="Create new package">
      </form>
      <div class="output">
        <p id="res">
          <%= res %>
        </p>
      </div>


      </div> <!-- content end -->

<% include footer %>